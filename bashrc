alias c="clear"
alias ta="tmux attach"
alias serve='python -m SimpleHTTPServer'

# Git stuff
alias gs="git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias ga="git add"
alias gcm="git commit"
alias gco="git checkout"
alias gg="git grep -n"
alias ggi="git grep -in"
alias gp="git pull"
alias gP="git push"
